{
  "permissions": {
    "allow": [
      "Bash(git add:*)",
      "Bash(mkdir:*)",
      "Bash(chmod:*)",
      "Bash(tree:*)",
      "Bash(git reset:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(./gradlew:*)",
      "Bash(gradle wrapper:*)",
      "Bash(source:*)",
